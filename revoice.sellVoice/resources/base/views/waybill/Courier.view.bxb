input-view {
  match {
    HanJinConcept
  }
  message {
    template ("택배사를 선택해주세요.")
  }
  render {
    form {
      elements {
        text-input {
          id (Courier)
          label (한진택배)
          type (HanJinConcept)
          max-length (50)
        }
      }
      on-submit {
        goal: InputWaybill
        value: viv.core.FormElement(Courier)
      }
    }
  }

}