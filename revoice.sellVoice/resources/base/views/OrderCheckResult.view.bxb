result-view {
 match {
     OrderCheckResultConcept (orderCheckResult)
   }
 render {
   if(size(orderCheckResult)>0){
     layout{
      layout-macro (order-list) {
        param (orderList) {
          expression (orderCheckResult)
        }
      }
    }
  }
  else{
    layout{      
      section{
        title{        
          template ("조회된 신규 주문이 없습니다.")
        }
      }      
    }
  }
  
  }
  
}