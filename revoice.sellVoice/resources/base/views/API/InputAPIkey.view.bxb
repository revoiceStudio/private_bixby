input-view {
  match {
    InputAPIkeyResultConcept
  }
  message {
    template ("운영중인 쇼핑몰의 API key를 입력해주세요. (현재는 11번가만 가능합니다.)")
  }
  render {
    form {
      elements {
        text-input {
          id (inputAPIkey)
          label (11번가 API key)
          type (APIkeyConcept)
          max-length (50)
        }
      }
      on-submit {
        goal: CheckAPIkey
        value: viv.core.FormElement(inputAPIkey)
      }
    }
  }

}