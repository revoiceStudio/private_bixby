    }else{
      template("#{value(detailResult[0].consumerName)}고객님 묶음 신규 주문 #{size(detailResult)}건이 있습니다.")
      template ("#{value(detailResult[0].consumerName)}고객님 묶음 신규 주문 #{size(detailResult)}건에 대한 검색 결과입니다.")
