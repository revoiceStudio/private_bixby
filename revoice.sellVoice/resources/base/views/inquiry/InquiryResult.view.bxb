result-view {
  match {
     InquiryResultConcept (inquiryResult)
  }
  render{
    if(size(inquiryResult)>0){
      layout{
        layout-macro (inquiry-list) {
          param (inquiryList) {
            expression (inquiryResult)
          }
        }
      }
    }else{
      layout{      
        section{
          title{        
            template ("신규 고객 문의가 없습니다.")
          }
        }      
      }
    }
  }
}