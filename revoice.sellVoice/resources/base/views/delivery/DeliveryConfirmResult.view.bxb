result-view {
 match {
    DeliveryConfirmResultConcept (DeliveryConfirmResult)
  }
  
 render {

     layout{
          
      section{
        title{        
          template ("전체 주문 #{size(DeliveryConfirmResult)}건 발주 처리")
        }
      }      
      if(size(DeliveryConfirmResult)>0){
        layout-macro (delivery-list) {
          param (deliveryList) {
            expression (DeliveryConfirmResult)
          }
        }
      }
    }
  }
}